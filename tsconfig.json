{
	"compilerOptions": {
	  "allowJs": true,
	  "noEmit": true,
	  "noImplicitAny": true,
	  "removeComments": true,
	  "preserveConstEnums": true,
	  "sourceMap": true,
	  "module": "commonjs",
	  "target": "es6",
	  "outDir": "out",
	  "lib": ["es6"]
	},
	"include": [
	  "**/*"
	],
	"exclude": [
	  "assets",
	  "node_modules",
	  "bower_components",
	  "jspm_packages",
	  "vscode-test"
	],
	"typeAcquisition": {
	  "enable": true
	}
 }